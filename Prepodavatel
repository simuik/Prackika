import java.util.Scanner;
public class Main {
    public static void main(String[] args) throws IOException {
        {

            Prepodavatel p1 = new Prepodavatel("Евгений", "Злобин", "Валентинович", "мужской", 38, "доктор биологических наук", 15, "профессор");
            Prepodavatel p2 = new Prepodavatel("Александр", "Ситовский", "Михайлович", "мужской", 45, "кандидат биологических наук", 20, "преподаватель");
            Prepodavatel p3 = new Prepodavatel("Дмитрий", "Амалин", "Алексеевич", "мужской", 33, "кандидакт гуманитарных наук", 10, "преподаватель");
            Prepodavatel p4 = new Prepodavatel("Людмила", "Мулакова", "Петровна", "женский", 41, "доктор биологических наук", 17, "декан");
            Prepodavatel p5 = new Prepodavatel("Владимир", "Казаков", "Степанович", "мужской", 29, "кандидат биолигический наук", 5, "аспирант");
            Prepodavatel p6 = new Prepodavatel("Надежда", "Зыкова", "Васильевна", "женский", 56, "кандидат биологических наук", 34, "профессор");

            Disciplins d1 = new Disciplins("Латинский язык", 48, 3001, 3002, 3003);
            Disciplins d2 = new Disciplins("Фармация", 70, 3002, 3009, 3004);
            Disciplins d3 = new Disciplins("Фармакология", 56, 3001, 3007, 3004);
            Disciplins d4 = new Disciplins("Фармакогнозия", 38, 3001, 3002, 3006);
            Disciplins d5 = new Disciplins("Химия", 54, 3002, 3007, 3006);
            Disciplins d6 = new Disciplins("Биотехнология", 22, 3002, 3004, 3007);

            //p1.printPrepodavatel();
            //d6.printDisciplins();
            Scanner k = new Scanner(System.in);
            System.out.println("Введите количество преподавателей(не больше 6)");
            int kk = k.nextInt();
            System.out.println("Количество преподавателей:" + k);
            Scanner v = new Scanner(System.in);
            System.out.println("Введите нужное количество ");
            int vv = v.nextInt();
            System.out.println("Нужное количество занятий у преподавателей:" + v);

            {
                for (int i = 0; i < kk; i++)

                    p1.printPrepodavatel();
                    d6.printDisciplins();
            }
            File file = new File("Xranenie");
            PrintWriter vyuvod = new PrintWriter(file);

            vyuvod.println("" + p1.printPrepodavatel());
            vyuvod.println("" + d6.printDisciplins());
                    vyuvod.close();
        }
    }
}

class Prepodavatel {
    private String name;
    private String familiya;
    private String otshestvo;
    private String gender;
    private int age;
    private String stepen;
    private int stazh;
    private String dolzhnost;

    public Prepodavatel(String x, String y, String c, String v, int b, String n,int m, String a){
        name = x;
        familiya = y;
        otshestvo = c;
        gender = v;
        age = b;
        stepen = n;
        stazh = m;
        dolzhnost = a;
    }
    public void printPrepodavatel(){
        System.out.println("ПРЕПОДАВАТЕЛЬ КАФЕДРЫ:");
        System.out.println("Фио:"+" "+familiya+" "+name+" "+otshestvo);
        System.out.println("Пол:"+gender);
        System.out.println("Возраст:"+age);
        System.out.println("Административная должность:"+dolzhnost);
        System.out.println("Стаж работы:"+stazh);
        System.out.println("Ученая степень:"+stepen);
    }
}

class Disciplins {
    private String naim;
    private int kolvochasov;
    private int number;
    private int number1;
    private int number2;

    public Disciplins(String q, int w, int e, int f,int j){
        naim = q;
        kolvochasov = w;
        number = e;
        number1 = f;
        number2 = j;
    }
    public void printDisciplins() {
        System.out.println("УЧЕБНАЯ ДИСЦИПЛИНА");
        System.out.println("Количество часов:" + kolvochasov);
        System.out.println("Наименование дисциплины:" + naim);
        System.out.println("Номер группы:" + number + number1 + number2);
    }
}
